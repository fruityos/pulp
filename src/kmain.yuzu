sub kmain(initJarPtr)
	local rdAddr, rdSize, blockSize

	_keys = 0
	_kbhead = 0
	_kbtail = 0

	kcls()
	kmemcpy(_cwd, "/", 2)

	kputs("Welcome to FruityOS!")
	sys_putch(10)

	heap_init()

	kputs("Unpacking initial jar...")
	sys_putch(10)
	kunpack(initJarPtr)

	kputs("Ready!")
	sys_putch(10)

	return 0
end
