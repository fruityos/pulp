sub sys_keyboard(scancode)
	local c, kb, scr
	kb = global_getKb()
	scr = global_getScr()

	c = kb_s2c(kb, scancode)
	if c != 0 then
		kb_push(kb, c)
		scr_putch(scr, c)
	end		
end
