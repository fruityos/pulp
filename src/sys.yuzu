sub sys_getch()
	local c, kb
	kb = global_getKb()
	; Spin while waiting for keyboard input
	while (c = kb_dequeue(kb)) == -1 do end
	return c
end

sub sys_putch(char)
	local scr
	scr = global_getScr()
	scr_putch(scr, char)
end

